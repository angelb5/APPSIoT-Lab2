<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.appsiot_lab2">

    <application
        android:name=".Lab2Application"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.APPSIoTLab2"
        tools:targetApi="31">
        <activity
            android:name=".Teclado.TecladoActualizarActivity"
            android:exported="false"
            android:parentActivityName=".Teclado.TecladoListActivity"
            />
        <activity
            android:name=".reporteActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            />
        <activity
            android:name=".Teclado.TecladoListActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            />
        <activity
            android:name=".Monitor.MonitorListActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            />
        <activity
            android:name=".Monitor.MonitorActualizarActivity"
            android:exported="false"
            android:parentActivityName=".Monitor.MonitorListActivity"
            />
        <activity
            android:name=".Teclado.TecladoNuevoActivity"
            android:exported="false"
            android:parentActivityName=".Teclado.TecladoListActivity"
            />
        <activity
            android:name=".Monitor.MonitorNuevoActivity"
            android:exported="false"
            android:parentActivityName=".Monitor.MonitorListActivity"/>
        <activity
            android:name=".Computadora.ComputadoraNuevoActivity"
            android:exported="false"
            android:parentActivityName=".Computadora.ComputadoraListaActivity"/>
        <activity
            android:name=".Computadora.ComputadoraListaActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".Computadora.ComputadoraActualizarActivity"
            android:exported="false"
            android:parentActivityName=".Computadora.ComputadoraListaActivity"/>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>